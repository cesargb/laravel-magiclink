{
    "name": "cesargb/laravel-magiclink",
    "description": "Create secure link for access to private data or login in Laravel without password",
    "keywords": [
        "laravel",
        "auth",
        "login",
        "link",
        "download",
        "file"
    ],
    "homepage": "https://github.com/cesargb/laravel-magiclink",
    "authors": [
        {
            "name": "Cesar Garcia",
            "email": "cesargb@gmail.com",
            "homepage": "https://github.com/cesargb",
            "role": "Developer"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "illuminate/auth": "^10.0|^11.0|^12.0",
        "illuminate/container": "^10.0|^11.0|^12.0",
        "illuminate/contracts": "^10.0|^11.0|^12.0",
        "illuminate/database": "^10.0|^11.0|^12.0",
        "laravel/serializable-closure": "^1.0|^2.0",
        "orchestra/testbench": "^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "MagicLink\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MagicLink\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "MagicLink\\MagicLinkServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.5|^11.5",
        "friendsofphp/php-cs-fixer": "^3.9"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
